	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
		"start": "remix-serve ./build/server/index.js",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"db:push": "prisma db push",
		"setup": "bun i -d && bun run db:push && bun run seed && bun run build",
		"upgrade": "bun i -d && bun run db:push && bun run build",
		"skin:setup": "bun i && cross-env MODE=skin bun run setup",
		"skin:upgrade": "bun i && cross-env MODE=skin bun run upgrade",
		"seed": "bun prisma/seed.ts",
		"typecheck": "tsc"
	},
